@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace BlazorActorApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" asp-append-version="true" />
    <link href="css/site.css" rel="stylesheet" asp-append-version="true" />
    <link href="BlazorActorApp.styles.css" rel="stylesheet" asp-append-version="true" />
    <link href="_content/Blazor.Bootstrap/blazor.bootstrap.css" rel="stylesheet" asp-append-version="true" />    

    <link rel="icon" type="image/png" href="favicon.png"/>    

    <!-- MudBlazor -->
    <link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="_content/MudBlazor.Markdown/MudBlazor.Markdown.min.css" rel="stylesheet" asp-append-version="true" />

    <!-- Diagrams Blazor-->
    <link href="_content/Z.Blazor.Diagrams/style.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="_content/Z.Blazor.Diagrams/default.styles.min.css" rel="stylesheet" asp-append-version="true" />


    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />
    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    
    <script src="_framework/blazor.server.js" asp-append-version="true"></script>
    <script src="_content/Blazor.Bootstrap/blazor.bootstrap.js" asp-append-version="true"></script>
    <script src="_content/MudBlazor/MudBlazor.min.js" asp-append-version="true"></script>
    <script src="_content/MudBlazor.Markdown/MudBlazor.Markdown.min.js" asp-append-version="true"></script>
    <script src="_content/Z.Blazor.Diagrams/script.min.js" asp-append-version="true"></script>
    <script src="_content/Blazor.Extensions.Canvas/blazor.extensions.canvas.js" asp-append-version="true"></script>
    <script src="/js/sse-sample.js" asp-append-version="true"></script>
</body>
</html>
